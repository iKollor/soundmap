{
    "name": "mapa-sonoro-guayaquil",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@9.0.0",
    "scripts": {
        "dev": "turbo dev",
        "dev:web": "turbo dev --filter @soundmap/web",
        "build": "turbo build",
        "lint": "turbo lint",
        "clean": "turbo clean && rm -rf node_modules",
        "db:push": "turbo db:push",
        "db:studio": "pnpm --filter @soundmap/database db:studio",
        "docker:up": "docker compose up -d",
        "docker:down": "docker compose down",
        "docker:logs": "docker compose logs -f"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@types/node": "^22.0.0",
        "eslint": "^9.0.0",
        "prettier": "^3.4.0",
        "turbo": "^2.3.0",
        "typescript": "^5.7.0",
        "typescript-eslint": "^8.54.0"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=9.0.0"
    }
}
